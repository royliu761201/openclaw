{
  "meta": {
    "description": "Multi-Agent Node Configuration for Mac 02 (GPU Compute)"
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "google/gemini-3-flash-preview"
      },
      "skipBootstrap": true,
      "compaction": {
        "mode": "safeguard"
      }
    },
    "list": [
      {
        "id": "agent-work",
        "skills": ["work-assistant"]
      },
      {
        "id": "agent-research",
        "skills": ["research-assistant"]
      }
    ]
  },
  "bindings": [
    {
      "agentId": "agent-work",
      "match": {
        "channel": "feishu",
        "accountId": "work"
      }
    },
    {
      "agentId": "agent-research",
      "match": {
        "channel": "feishu",
        "accountId": "research"
      }
    }
  ],
  "tools": {
    "web": {
      "search": {
        "enabled": true,
        "provider": "tavily"
      }
    }
  },
  "channels": {
    "feishu": {
      "enabled": true,
      "domain": "feishu",
      "connectionMode": "websocket",
      "dmPolicy": "open",
      "groupPolicy": "allowlist",
      "allowFrom": ["*"],
      "accounts": {
        "work": {
          "appId": "cli_a9282264a3389bc4",
          "appSecret": "UVIF9RU3pwSklF7MIltryfsNnKUZ7Oys"
        },
        "research": {
          "appId": "cli_a91f1b494178dcbd",
          "appSecret": "feCLN4JX4Khy49PKBbYtMeQNmfwft2vC"
        }
      }
    }
  },
  "gateway": {
    "enabled": false
  },
  "plugins": {
    "entries": {
      "feishu": {
        "enabled": true
      }
    }
  }
}
